calcul_M_g_hat_test_sample <- function(g_hat_on_Z_test, Omega, n_test, Y_test){
M = 0
for (i in 1:n_test){
for (j in 1:n_test){
M = M + (Y_test[i] - g_hat_on_Z_test[i])*(Y_test[j] - g_hat_on_Z_test[j])*Omega[i,j]
}
return(M/(n_test**2))
}}
